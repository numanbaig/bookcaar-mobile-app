{
  "expo": {
    "name": "bookcaar",
    "plugins": [
      [
        "expo-image-picker",
        {
          "locationWhenInUsePermission": "Allow Bookcaar to access your location"
        }
      ]
    ],
    "slug": "bookcaar",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#FFFFFF"
      }
    },
    "web": {
      "favicon": "./assets/favicon.png"
    }
  },
  "android": {
    "config": {
      "googleMaps": {
        "apiKey": "AIzaSyCLA-9u2HEIBjxpFeS2QxeMTM46_ENNOZI"
      }
    }
  }
}
